{
  "name": "meals",
  "version": "1.0.0",
  "author": "Tiago da Rosa Santos",
  "private": true,
  "scripts": {
    "build": "npm run build-ts && npm run copy-assets",
    "build-ts": "tsc",
    "copy-assets": "mkdir -p dist/public && cp src/public/*.yaml dist/public/",
    "debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run ts\" \"npm run serve-debug\"",
    "openapi": "del-cli ./src/api && openapi -i ./src/public/meals-v1.openapi.yaml -o ./src/api -c axios",
    "serve": "node --preserve-symlinks dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "ts": "tsc -w"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "async": "^3.2.2",
    "axios": "^0.27.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.8",
    "express-validator": "^6.14.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^8.7.0",
    "http-status": "^1.5.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^9.0.0",
    "lusca": "^1.6.1",
    "mongodb": "^4.17.2",
    "node-fetch": "^2.6.1",
    "serialize-error": "^8.0.0",
    "source-map-support": "^0.5.19",
    "swagger-routes-express": "^3.3.2",
    "swagger-ui-express": "^4.5.0",
    "ts-nested-error": "^1.2.1",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.12",
    "@types/deep-equal": "^1.0.1",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.30",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lusca": "^1.6.2",
    "@types/node": "^18.7.1",
    "@types/node-fetch": "^2.6.2",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/shelljs": "^0.8.8",
    "@types/source-map-support": "^0.5.3",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "concurrently": "^7.3.0",
    "deep-equal": "^2.1.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.7",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "bundleDependencies": true,
  "files": [
    "dist/*"
  ]
}
